# N8N Configuration
N8N_HOST=your-ngrok-domain.ngrok-free.app
N8N_PORT=443
N8N_PROTOCOL=https
WEBHOOK_TUNNEL_URL=https://your-ngrok-domain.ngrok-free.app

# N8N Authentication (optional but recommended)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-secure-password
N8N_ENCRYPTION_KEY=your-secret-encryption-key-here

# Timezone
GENERIC_TIMEZONE=UTC

# Ngrok Configuration
NGROK_AUTHTOKEN=your-ngrok-auth-token
NGROK_DOMAIN=your-ngrok-domain.ngrok-free.app

# OpenAI Configuration
OPENAI_APIKEY=your-openai-api-key-here
